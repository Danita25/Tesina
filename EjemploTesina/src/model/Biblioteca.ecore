<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="Biblioteca" nsURI="http://www.eclipse.org/ocl/examples/Biblioteca.ecore"
    nsPrefix="Biblioteca">
  <eAnnotations source="http://www.eclipse.org/OCL/Import">
    <details key="ecore" value="http://www.eclipse.org/emf/2002/Ecore#/"/>
  </eAnnotations>
  <eClassifiers xsi:type="ecore:EClass" name="Biblioteca">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="nombre" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="direccion" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="socios" upperBound="-1"
        eType="#//Socio"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="libros" lowerBound="1"
        upperBound="-1" eType="#//Libro"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Libro">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="titulo" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="editorial" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="anioDeEdicion" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="TipoDeLibro" eType="#//TipoDeLibro"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="autor" lowerBound="1" eType="#//Autor"
        eOpposite="#//Autor/obras"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="copias" lowerBound="1"
        upperBound="-1" eType="#//Copia" eOpposite="#//Copia/libro"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Copia">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="numeroDeCopia" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="estadoDeLaCopia" eType="#//EstadoDeLaCopia"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="libro" lowerBound="1" eType="#//Libro"
        eOpposite="#//Libro/copias"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="EstadoDeLaCopia">
    <eLiterals name="biblioteca" value="1"/>
    <eLiterals name="prestado" value="2"/>
    <eLiterals name="retrasado" value="3"/>
    <eLiterals name="reparacion" value="4"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="TipoDeLibro">
    <eLiterals name="novela" value="1"/>
    <eLiterals name="teatro" value="2"/>
    <eLiterals name="poesia" value="3"/>
    <eLiterals name="ensayo" value="4"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Autor">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="nombre" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="nacionalidad" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="fechaDeNacimiento" eType="#//Date"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="obras" lowerBound="1" upperBound="-1"
        eType="#//Libro" eOpposite="#//Libro/autor"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Prestamo">
    <eOperations name="finalizarPrestamo" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean">
      <eParameters name="fechaDeDevolucion" eType="#//Date"/>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="fechaDeInicio" eType="#//Date"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="fechaDeFin" eType="#//Date"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="fechaDeDevolucion" eType="#//Date"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="copia" lowerBound="1" eType="#//Copia"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Socio">
    <eOperations name="solicitarPrestamo" eType="#//Prestamo">
      <eParameters name="libro" eType="#//Libro"/>
    </eOperations>
    <eOperations name="generarMulta" eType="#//Multa">
      <eParameters name="prestamo" eType="#//Prestamo"/>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="numeroDeSocio" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="nombreCompleto" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="direccion" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="telefono" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="fechaDeNacimiento" eType="#//Date"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="prestamos" upperBound="-1"
        eType="#//Prestamo"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="multas" upperBound="-1"
        eType="#//Multa"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="activo" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBooleanObject"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Multa">
    <eOperations name="finalizarMulta" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean">
      <eParameters name="fechaFin" eType="#//Date"/>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="fechaDeInicio" eType="#//Date"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="fechaDeFin" eType="#//Date"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="prestamo" lowerBound="1"
        eType="#//Prestamo" transient="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Date">
    <eOperations name="esAnterior">
      <eParameters name="date" eType="#//Date"/>
    </eOperations>
    <eOperations name="esIgual">
      <eParameters name="date" eType="#//Date"/>
    </eOperations>
    <eOperations name="desdeYMD" eType="#//Date">
      <eParameters name="dia" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger"/>
      <eParameters name="mes" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger"/>
      <eParameters name="anio" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger"/>
    </eOperations>
    <eOperations name="esPosterior">
      <eParameters name="t" eType="#//Date"/>
    </eOperations>
    <eOperations name="diferenciaEnAnios" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger">
      <eParameters name="date" eType="#//Date"/>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="anio" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="mes" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="dia" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBigInteger"/>
  </eClassifiers>
</ecore:EPackage>
