<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    name="Biblioteca"
    nsURI="Tesis.Biblioteca.ecore"
    nsPrefix="Biblio">
  <eAnnotations
      source="http://www.eclipse.org/OCL/Import">
    <details
        key="ecore"
        value="http://www.eclipse.org/emf/2002/Ecore"/>
  </eAnnotations>
  <eClassifiers
      xsi:type="ecore:EClass"
      name="Biblioteca">
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="direccion"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="libros"
        ordered="false"
        upperBound="-1"
        eType="#//Libro"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="fechaDeApertura"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EClass"
      name="Libro">
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="titulo"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="editorial"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="anioDeEdicion"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="tipo"
        lowerBound="1"
        eType="#//TipoDeLibro"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="copias"
        ordered="false"
        lowerBound="1"
        upperBound="-1"
        eType="#//Copia"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="autor"
        lowerBound="1"
        eType="#//Autor"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="activo"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBooleanObject"
        defaultValueLiteral="true"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="fechaDeEscritura"
        ordered="false"
        unique="false"
        lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EClass"
      name="Copia">
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="numeroDeCopia"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt">
      <eAnnotations
          source="http://schema.omg.org/spec/MOF/2.0/emof.xml#Property.oppositeRoleName">
        <details
            key="body"
            value="Prestamo"/>
        <details
            key="unique"
            value="false"/>
        <details
            key="upper"
            value="*"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="estado"
        lowerBound="1"
        eType="#//EstadoDeLaCopia"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="fechaDeAdquisicion"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EClass"
      name="Prestamo">
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="fechaInicio"
        lowerBound="1"
        eType="#//Date"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="fechaFin"
        lowerBound="1"
        eType="#//Date"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="ejemplar"
        lowerBound="1"
        eType="#//Copia"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="solicitante"
        lowerBound="1"
        eType="#//Socio"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="fechaDeDevolucion"
        lowerBound="1"
        eType="#//Date"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EClass"
      name="Autor">
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="nombre"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="nacionalidad"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="fechaDeNacimiento"
        lowerBound="1"
        eType="#//Date"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="obras"
        lowerBound="1"
        upperBound="-1"
        eType="#//Libro"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EClass"
      name="Socio">
    <eOperations
        name="solicitarEjemplar"
        lowerBound="1"
        eType="#//Prestamo">
      <eParameters
          name="numeroDeCopia"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eOperations>
    <eOperations
        name="devolverEjemplar"
        eType="#//Prestamo">
      <eParameters
          name="numeroDeCopia"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eParameters
          name="fechaActual"
          lowerBound="1"
          eType="#//Date"/>
    </eOperations>
    <eOperations
        name="multar">
      <eParameters
          name="prestamo"
          eType="#//Prestamo"/>
      <eParameters
          name="dias"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eOperations>
    <eOperations
        name="uniqueID"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="numeroDeSocio"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt">
      <eAnnotations
          source="http://schema.omg.org/spec/MOF/2.0/emof.xml#Property.oppositeRoleName">
        <details
            key="body"
            value="Multa"/>
        <details
            key="unique"
            value="false"/>
        <details
            key="upper"
            value="*"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="nombreCompleto"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations
          source="http://schema.omg.org/spec/MOF/2.0/emof.xml#Property.oppositeRoleName">
        <details
            key="body"
            value="Multa"/>
        <details
            key="unique"
            value="false"/>
        <details
            key="upper"
            value="*"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="telefono"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations
          source="http://schema.omg.org/spec/MOF/2.0/emof.xml#Property.oppositeRoleName">
        <details
            key="body"
            value="Multa"/>
        <details
            key="unique"
            value="false"/>
        <details
            key="upper"
            value="*"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="direccion"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="fechaDeNacimiento"
        lowerBound="1"
        eType="#//Date"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="prestamos"
        upperBound="-1"
        eType="#//Prestamo"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="multas"
        upperBound="-1"
        eType="#//Multa"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="edad"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EClass"
      name="Multa">
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="fechaInicio"
        lowerBound="1"
        eType="#//Date"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="fechaFin"
        lowerBound="1"
        eType="#//Date"/>
    <eStructuralFeatures
        xsi:type="ecore:EReference"
        name="prestamo"
        lowerBound="1"
        eType="#//Prestamo"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EEnum"
      name="TipoDeLibro">
    <eLiterals
        name="Lirico"/>
    <eLiterals
        name="Epico"
        value="1"/>
    <eLiterals
        name="Narrativo"
        value="2"/>
    <eLiterals
        name="Dramatico"
        value="3"/>
    <eLiterals
        name="Didactico"
        value="4"/>
    <eLiterals
        name="Terror"
        value="5"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EEnum"
      name="EstadoDeLaCopia">
    <eLiterals
        name="Bueno"/>
    <eLiterals
        name="Regular"
        value="1"/>
    <eLiterals
        name="Malo"
        value="2"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EClass"
      name="Date">
    <eOperations
        name="isBefore"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBooleanObject">
      <eParameters
          name="t"
          eType="#//Date"/>
    </eOperations>
    <eOperations
        name="isEqual"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBooleanObject">
      <eParameters
          name="t"
          eType="#//Date"/>
    </eOperations>
    <eOperations
        name="fromYMD"
        eType="#//Date">
      <eParameters
          name="m"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EIntegerObject"/>
      <eParameters
          name="k"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EIntegerObject"/>
      <eParameters
          name="y"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EIntegerObject"/>
    </eOperations>
    <eOperations
        name="isAfter"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBooleanObject">
      <eParameters
          name="t"
          eType="#//Date"/>
    </eOperations>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="year"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EIntegerObject"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="month"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EIntegerObject"/>
    <eStructuralFeatures
        xsi:type="ecore:EAttribute"
        name="day"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EIntegerObject"/>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EEnum"
      name="Tipo">
    <eLiterals
        name="femenino"/>
    <eLiterals
        name="masculino"
        value="1"/>
  </eClassifiers>
</ecore:EPackage>
